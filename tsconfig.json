{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "dist",
    "rootDir": ".",
    "baseUrl": "./",
    "paths": {
      "~src/*": [
          "src/*"
      ],
      "~algoSdkMakeAsset/*": [
          "algoSdkMakeAsset/*"
      ],
      "~algoSdkCreateAsset/*": [
          "algoSdkCreateAsset/*"
      ],
    },
    "sourceMap": true,
    "resolveJsonModule": true,
    "strict": false,
    "plugins": [
      { "transform": "ts-reflection/transformer" },
      { "transform": "ts-type-checked/transformer" }   
    ]
  }
}
